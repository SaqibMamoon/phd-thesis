isbn = {9781510810587},
keywords = {dblp},
pages = {2285--2294},
publisher = {JMLR.org},
series = {JMLR Proceedings},
title = {{Compressing Neural Networks with the Hashing Trick}},
url = {http://arxiv.org/abs/1504.04788},
volume = {37},
year = {2015}
}
