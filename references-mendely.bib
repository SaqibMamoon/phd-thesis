author = {Larsen, Kasper Green and Nelson, Jelani},
eprint = {1609.02094},
isbn = {0001415123},
journal = {arXiv preprint arXiv:1609.02094},
number = {1},
pages = {1--11},
title = {{Optimality of the Johnson-Lindenstrauss Lemma}},
