keywords = {CT,Entanglement,auto-context,decision forests,segmentation},
pages = {184--196},
pmid = {21761656},
title = {{Entangled decision forests and their application for semantic segmentation of CT images}},
volume = {6801 LNCS},
year = {2011}
}
